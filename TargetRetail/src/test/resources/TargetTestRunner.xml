<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Target Automation Test" parallel="false">
    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="Browserstack"/>
    <parameter name="OS" value="Win"/>
    <parameter name="OS_Version" value="8"/>
    <parameter name="browser" value="Chrome"/>
    <parameter name="Browser_Version" value="53.0"/>
    <parameter name="url" value="https://www.target.com/"/>
    <test name="Targets Test">
        <classes>
            <class name="testTargetRetail.testHomePage"/>
            <class name="testTargetRetail.testSigninPage"/>
            <class name="testTargetRetail.testCreateAccount"/>
        </classes>
    </test>
</suite>