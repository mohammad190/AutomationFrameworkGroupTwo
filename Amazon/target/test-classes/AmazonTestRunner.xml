<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Amazon Automation" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="OS" value="Win"/>
    <parameter name="OS_Version" value="10"/>
    <parameter name="Browser_Version" value="53.0"/>
    <parameter name="browser" value="Chrome"/>
    <parameter name="url" value="https://amazon.com"/>

    <test name="Amazon Automation Test">
        <classes>
            <class name="testHome.TestDepartment"/>
            <class name="testHome.TestPrime"/>
            <class name="testHome.TestTodaysDeals"/>
            <class name="testHome.TestGift"/>

        </classes>
    </test>

</suite>